@using SmartLMS.Dominio.Entidades;
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="x-ua-compatible" content="IE=10">
    <title>Smart LMS - @Parametro.PROJETO - @ViewBag.Title</title>
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <link rel="stylesheet" href="http://fonts.googleapis.com/css?family=Roboto:300,400,500,700" type="text/css">
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <base href="@HttpContext.Current.Request.Url.GetLeftPart(UriPartial.Authority)@Url.Content("~/")" />
</head>
<body>
   
    @Html.Action("ExibirCabecalho", "Usuario")
  
    <div class="container">
        @RenderBody()
    </div>
    
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    <script>
        SmartLMS.IdUsuarioLogado = '@ViewBag.IdUsuarioLogado';
    </script>
    @RenderSection("scripts", required: false)

    @Html.Partial("_Alert")
</body>
</html>
